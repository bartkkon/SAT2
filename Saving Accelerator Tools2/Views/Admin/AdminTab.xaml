<Page x:Class="Saving_Accelerator_Tools2.Views.Admin.AdminTab"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
      xmlns:local="clr-namespace:Saving_Accelerator_Tools2.Views.Admin"
      mc:Ignorable="d"
      d:DesignHeight="450"
      d:DesignWidth="800"
      Title="AdminTab">

    <Grid>
        <TabControl>
            <TabItem Header="Amounts">
                <Frame Source="DataTab.xaml" />
            </TabItem>
            <TabItem Header="Users">
                <Frame Source="UsersTab.xaml" />
            </TabItem>
            <TabItem Header="Others">
                <Frame Source="OthersTab.xaml" />
            </TabItem>
        </TabControl>
    </Grid>
</Page>
