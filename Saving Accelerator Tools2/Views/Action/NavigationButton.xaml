<UserControl x:Class="Saving_Accelerator_Tools2.Views.Action.NavigationButton"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:model="clr-namespace:Saving_Accelerator_Tools2.ViewModels.Action"
             xmlns:local="clr-namespace:Saving_Accelerator_Tools2.Views.Action"
             mc:Ignorable="d" 
             d:DesignHeight="50" d:DesignWidth="100">
    <UserControl.DataContext>
        <model:NavigationButtonViewModel/>
    </UserControl.DataContext>
    <Grid Height="50" Width="100" MinWidth="100" MinHeight="50" MaxWidth="100" MaxHeight="50">
        <Button Content="Save" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="80" Command="{Binding Path=SaveAction}"/>

    </Grid>
</UserControl>
