<UserControl x:Class="Saving_Accelerator_Tools2.Views.Action.QuantityEstymation"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Saving_Accelerator_Tools2.Views.Action"
             xmlns:model="clr-namespace:Saving_Accelerator_Tools2.ViewModels.Action"
             mc:Ignorable="d" 
             d:DesignHeight="90" d:DesignWidth="120" Width="120" MinWidth="120" MaxWidth="120" Height="90" MinHeight="90" MaxHeight="90">
    <UserControl.DataContext>
        <model:QuantityEstymationModelView/>
    </UserControl.DataContext>
    <Grid>
        <TextBox HorizontalAlignment="Left" Height="20" Margin="10,39,0,0" Text="{Binding Path=Value, Mode=TwoWay}" TextWrapping="Wrap" VerticalAlignment="Top" Width="80" KeyDown="TextBox_KeyDown"/>
        <Label Content="%" HorizontalAlignment="Left" Height="30" Margin="90,40,0,0" VerticalAlignment="Top" Width="25" FontSize="16"/>
        <Label Content="Quantity Estymation:" HorizontalAlignment="Center" Height="30" Margin="0,5,0,0" VerticalAlignment="Top" Width="120"/>

    </Grid>
</UserControl>
